{
  "extends": "@octokit/tsconfig",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "module": "node16",
    "moduleResolution": "node16",
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true
  }
}
