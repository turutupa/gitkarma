{
  "name": "gitkarma",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "server": "node app.js",
    "dev": "clear && bash ../scripts/gitkarma.sh backend && npx tsx watch src/app.ts",
    "type-check": "tsc --noEmit",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && node postbuild.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "node typeorm-cli.js -f ormconfig.json",
    "migration:generate": "node typeorm-cli.js migration:generate",
    "migration:create": "node typeorm-cli.js migration:create",
    "migration:run": "node typeorm-cli.js migration:run",
    "migration:revert": "node typeorm-cli.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@octokit/rest": "^21.1.0",
    "@octokit/types": "^13.8.0",
    "@types/express": "^5.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "octokit": "^4.1.1",
    "pg": "^8.13.3",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "tigerbeetle-node": "0.16.27",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@octokit/tsconfig": "^4.0.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.9",
    "@types/pg": "^8.11.11",
    "glob": "^11.0.1",
    "smee-client": "^2.0.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
